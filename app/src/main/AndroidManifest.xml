<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.fernandescruz.services">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="br.com.fernandescruz.services.AtividadesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="br.com.fernandescruz.services.AlarmReceiver" />

        <activity android:name="br.com.fernandescruz.services.MainActivity" />
        <activity android:name="br.com.fernandescruz.services.ServicesActivity" />
        <activity android:name="br.com.fernandescruz.services.LoginActivity" />
        <activity android:name="br.com.fernandescruz.services.BoundActivity"></activity>

        <service android:name="br.com.fernandescruz.services.Loggers" android:enabled="true" />
        <service android:name="br.com.fernandescruz.services.BoundService" android:enabled="true" />
    </application>

</manifest>